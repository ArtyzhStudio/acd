{
    "manifest_version": 3,
    "name": "Project InfoSec",
    "description": "Finds most likely url from history",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "history",
        "tabs",
        "activeTab",
        "background",
        "scripting",
        "storage",
        "unlimitedStorage"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}